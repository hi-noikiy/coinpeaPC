{
	"extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
	"linterOptions": {
		"exclude": [
			"config/**/*.js",
			"node_modules/**/*.ts",
			"src/**/*.js",
			"src/**/*.ts",
			"src/**/*.tsx",
			"src/**/*.ts",
			"config-overrides.js",
			"build"
		]
	},
	"rules": {
		"no-console":false,
		"no-bitwise":false
		
	}
}
